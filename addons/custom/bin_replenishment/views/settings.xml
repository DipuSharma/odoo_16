<?xml version="1.0" encoding="utf-8"?>
<odoo>
<record id="bin-replenishment-view-form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.br</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="70"/>
        <field name="inherit_id" ref="base.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[hasclass('settings')]" position="inside" >
                <div class="app_settings_block" data-string="Bin_replenishment" string="Bin Replenishment">
                    <h2>Bin Replenishment</h2>
                    <div class="row mt16 o_settings_container" name="employee_rights_setting_container">
                        <div class="col-12 col-lg-6 o_setting_box">
                            <div class="o_setting_right_pane">
                                <label for="schedule_assign_user_id"  class="o_light_label"/>-
                                <field name="schedule_assign_user_id" />
                            </div>
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>
</odoo>